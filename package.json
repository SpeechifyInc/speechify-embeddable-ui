{
  "name": "@speechify/embeddable-ui",
  "version": "0.0.8",
  "description": "Official Speechify Embeddable UI Components Library",
  "type": "module",
  "main": "./dist/speechify-embeddable-ui.umd.cjs",
  "module": "./dist/speechify-embeddable-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/speechify-embeddable-ui.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/speechify-embeddable-ui.umd.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/speechify/embeddable-ui.git"
  },
  "keywords": [
    "speechify",
    "text-to-speech",
    "audio",
    "player",
    "web-components"
  ],
  "author": "Speechify",
  "license": "MIT",
  "dependencies": {
    "@speechify/api-sdk": "^2.3.0"
  }
}
